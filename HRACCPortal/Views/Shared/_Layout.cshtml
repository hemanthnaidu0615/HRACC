        let inactivityTime = function () {
            let logoutTimer;
            let warningTimer;
            let countdownInterval;
            let countdown = 30;

            // Create modal HTML dynamically
            const modalHTML = `
                    <div class="modal fade" id="sessionTimeoutModal" tabindex="-1" role="dialog" aria-labelledby="sessionTimeoutLabel" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered" role="document">
                            <div class="modal-content shadow rounded">
                                <div class="modal-header bg-warning text-dark">
                                    <h5 class="modal-title" id="sessionTimeoutLabel">Session Expiring</h5>
                                </div>
                                <div class="modal-body">
                                    <p>Your session will expire in <strong><span id="timeoutCountdown">30</span> seconds</strong> due to inactivity.</p>
                                    <p>Click "Stay Logged In" to continue your session.</p>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" id="stayLoggedInBtn" class="btn btn-success" data-bs-dismiss="modal">Stay Logged In</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    `;
            document.body.insertAdjacentHTML("beforeend", modalHTML);

            function resetTimer() {
                clearTimeout(logoutTimer);
                clearTimeout(warningTimer);
                clearInterval(countdownInterval);
                $('#sessionTimeoutModal').modal('hide');

                // Warning after 4.5 minutes
                warningTimer = setTimeout(showWarningModal, 270000);

                // Auto logout after 5 minutes
                logoutTimer = setTimeout(logout, 300000);
            }

            function logout() {
                window.location.href = '/Account/LogOff';
            }

            function showWarningModal() {
                countdown = 30;
                document.getElementById("timeoutCountdown").textContent = countdown;
                $('#sessionTimeoutModal').modal('show');

                countdownInterval = setInterval(() => {
                    countdown--;
                    document.getElementById("timeoutCountdown").textContent = countdown;
                    if (countdown <= 0) {
                        clearInterval(countdownInterval);
                    }
                }, 1000);
            }

            // Event binding
            document.addEventListener("mousemove", resetTimer);
            document.addEventListener("keypress", resetTimer);
            document.addEventListener("scroll", resetTimer);
            document.addEventListener("touchstart", resetTimer);
            window.addEventListener("load", resetTimer);

            // Delegate click handler since button is dynamically added
            document.addEventListener("click", function (e) {
                if (e.target && e.target.id === "stayLoggedInBtn") {
                    resetTimer();
                }
            });

            resetTimer();
        };

